@model AccountViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>UoW Personel Prim Transferi</h3>

<form method="post">
    <label>Gönderen ID</label>
    <input type="text" asp-for="SenderID" class="form-control" />
    <br />
    <label>Alıcı ID</label>
    <input type="text" asp-for="ReceiverID" class="form-control" />
    <br />
    <label>Miktar</label>
    <input type="text" asp-for="Amount" class="form-control" />
    <br />
    <button  class="btn btn-outline-success">Hesaba Aktar</button>
</form>
